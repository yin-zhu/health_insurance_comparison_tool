<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All insurance plans
    </h1>

    <a href="/insurance_plans/new" class="btn btn-block btn-outline-secondary">
      Add a new insurance plan
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Coinsurance paid by patient
        </th>

        <th>
          Deductible
        </th>

        <th>
          Monthly premium
        </th>

        <th>
          Plan name
        </th>

        <th>
          Comp table
        </th>

        <th>
          Age group
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @insurance_plans.each do |insurance_plan| %>
      <tr>
        <!-- Display insurance_plan.id start -->
        <td>
          <%= insurance_plan.id %>
        </td>
        <!-- Display insurance_plan.id end -->

        <!-- Display insurance_plan.coinsurance_paid_by_patient start -->
        <td>
          <%= insurance_plan.coinsurance_paid_by_patient %>
        </td>
        <!-- Display insurance_plan.coinsurance_paid_by_patient end -->

        <!-- Display insurance_plan.deductible start -->
        <td>
          <%= insurance_plan.deductible %>
        </td>
        <!-- Display insurance_plan.deductible end -->

        <!-- Display insurance_plan.monthly_premium start -->
        <td>
          <%= insurance_plan.monthly_premium %>
        </td>
        <!-- Display insurance_plan.monthly_premium end -->

        <!-- Display insurance_plan.plan_name start -->
        <td>
          <%= insurance_plan.plan_name %>
        </td>
        <!-- Display insurance_plan.plan_name end -->

        <!-- Display insurance_plan.comp_table_id start -->
        <td>
          <%= insurance_plan.comp_table_id %>
        </td>
        <!-- Display insurance_plan.comp_table_id end -->

        <!-- Display insurance_plan.age_group start -->
        <td>
          <%= insurance_plan.age_group %>
        </td>
        <!-- Display insurance_plan.age_group end -->

        <!-- Display insurance_plan.created_at start -->
        <td>
          <%= time_ago_in_words(insurance_plan.created_at) %> ago
        </td>
        <!-- Display insurance_plan.created_at end -->

        <!-- Display insurance_plan.updated_at start -->
        <td>
          <%= time_ago_in_words(insurance_plan.updated_at) %> ago
        </td>
        <!-- Display insurance_plan.updated_at end -->

        <td>
          <a href="/insurance_plans/<%= insurance_plan.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>

    <div class="row mb-3">
      <div class="col-md-12">
        <%= paginate @insurance_plans, theme: "twitter-bootstrap-4" %>
      </div>
    </div>

  </div>
</div>
